{
  "name": "blpraveen/moloquent",
  "description": "A MongoDB based Eloquent model and Query builder for Laravel (Moloquent)",
  "keywords": [
    "laravel",
    "eloquent",
    "mongodb",
    "mongo",
    "database",
    "model",
    "moloquent"
  ],
  "homepage": "https://github.com/blpraveen/moloquent",
  "authors": [
    {
      "name": "B L Praveen"
    }
  ],
  "license": "MIT",
  "require": {
    "blpraveen/monolog":"^2.0",
    "illuminate/support": "^8.2",
    "illuminate/container": "^8.2",
    "illuminate/database": "^8.2",
    "illuminate/events": "^8.0",
    "mongodb/mongodb": "^1.0"
  },
  "require-dev": {
    "laravel/passport": "^10.1",
    "php": "^7.3|^8.0",
    "orchestra/testbench": "^6.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4" :{
      "Moloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/models",
      "tests/seeds"
    ]
  }
}
